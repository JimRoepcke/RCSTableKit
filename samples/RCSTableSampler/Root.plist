{
	nib = RCSTableViewPlain;
	controllerClass = RootViewController;
	allowsSelection = YES;
	staticTitle = "RCSTable Sampler";
	displaySections = (
		hello,
		world,
	);
	sections = {
		hello = {
			displayRows = (
				sub,
				helloWorld,
			);
			rows = {
				sub = {
					backgroundColor = subBackgroundColor;
					staticCellStyle = subtitle;
					staticText = RCSTableKit;
					staticDetailText = "Sample app";
				};
				helloWorld = {
					textSelector = "text:";
					pushConfiguration = Root;
					staticAccessoryType = disclosureIndicator;
				};
			};
		};
		world = {
			displayRows = (
				first,
				last,
			);
			staticTitle = Person;
			rows = {
				first = {
					staticCellStyle = value2;
					staticText = "First Name";
					staticDetailText = John;
				};
				last = {
					staticCellStyle = value2;
					staticText = "Last Name";
					staticDetailText = Appleseed;
				};
			};
		};
	};
}