{
	nib = RCSTableViewPlain;
	controller = RootViewController;
	allowsSelection = YES;
	title = "RCSTable Sampler";
	displaySections = (
		hello,
		world,
	);
	sections = {
		hello = {
			displayRows = (
				sub,
				helloWorld,
			);
			rows = {
				sub = {
					backgroundColor = subBackgroundColor;
					cellStyle = subtitle;
					text = RCSTableKit;
					detailText = "Sample app";
				};
				helloWorld = {
					"text:sel" = "text:";
					pushConfiguration = Root;
					accessoryType = disclosureIndicator;
				};
			};
		};
		world = {
			displayRows = (
				first,
				last,
			);
			title = Person;
			rows = {
				first = {
					cellStyle = value2;
					text = "First Name";
					detailText = John;
				};
				last = {
					cellStyle = value2;
					text = "Last Name";
					detailText = Appleseed;
				};
			};
		};
	};
}